{
  "name": "@chkp/quantum-gaia-mcp",
  "version": "1.0.1",
  "bin": {
    "quantum-gaia-mcp": "dist/index.js"
  },
  "description": "Official MCP server for Check Point Quantum Gaia",
  "repository": {
    "type": "git",
    "url": "https://github.com/CheckPointSW/mcp-servers.git",
    "directory": "packages/gaia"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run build:bundle && npx shx chmod +x ./dist/index.js && npx shx cp src/server-config.json dist/",
    "build:bundle": "node ../../bundle-mcp.js",
    "build:tsc": "npx tsc && npx shx chmod +x ./dist/index.js && npx shx cp src/server-config.json dist/",
    "dev": "tsc --watch",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.12.1",
    "commander": "^13.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@chkp/mcp-utils": "*",
    "@chkp/quantum-infra": "*",
    "@types/commander": "^2.12.0",
    "@types/node": "^20.0.0",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
